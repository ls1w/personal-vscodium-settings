{
    "window.titleBarStyle": "custom",
    "terminal.integrated.fontFamily": "MesloLGS NF, Regular",
    "terminal.integrated.profiles.linux": {
        "bash": {
        "path": "/usr/bin/flatpak-spawn",
        "args": ["--host", "--env=TERM=xterm-256color", "bash"]
        },
        "zsh": {
        "path": "/usr/bin/flatpak-spawn",
        "args": ["--host", "--env=TERM=xterm-256color", "zsh"],
        },
        "Flatpak": {
        "path": "/bin/bash"
        }
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "workbench.productIconTheme": "adwaita",
    "workbench.iconTheme": "material-icon-theme",
    "syncSettings.confirmSync": true,
    "vim.useSystemClipboard": true,
    "vim.replaceWithRegister": true,
    "vim.statusBarColorControl": true,
    "workbench.preferredDarkColorTheme": "Adwaita Dark",
    "workbench.preferredLightColorTheme": "Adwaita Light",
    "workbench.colorCustomizations": {
        "statusBar.background": "#5f0000",
        "statusBar.noFolderBackground": "#5f0000",
        "statusBar.debuggingBackground": "#5f0000"
    },
    "syncSettings.hostname": "ls1w",
    "editor.accessibilitySupport": "off",
    "window.menuBarVisibility": "compact",
    "workbench.colorTheme": "Adwaita Dark",
    "window.commandCenter": true,
    "terminal.integrated.localEchoExcludePrograms": [
        "nvim",
        "vi",
        "nano",
        "tmux",
        "vim"
    ],
}